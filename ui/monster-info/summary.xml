<?xml version="1.0" encoding="UTF-8"?>
<themes>
    <theme name="monster-frame" ref="resizableframe">


        <theme name="itemplate" ref="-defaults">
            <theme name="label" ref="-defaults">
                <param name="font"><font>alphabeta-black</font></param>
                <param name="textAlignment"><enum type="alignment">LEFT</enum></param>
            </theme>
            <theme name="label2" ref="-defaults">
                <param name="font"><font>alphabeta-black</font></param>
                <param name="border"><border>0,10,1,0</border></param>
            </theme>
        </theme>

        <theme name="monsterframe-ball" ref="button" allowWildcard="true">
            <param name="border"><border>0,0,0,0</border></param>
            <param name="background"><image>none</image></param>
        </theme>


    </theme>
</themes>